miscScripts:NewButton("Time Modifier", "Sets The In-Game 'Time' Leaderstat To 99,999 -- (May Reset After Leaderstat Is Updated)", function()

    while true do
    local player = game.Players.LocalPlayer
    local leaderstats = player:FindFirstChild("leaderstats")
    if leaderstats then
        local time = leaderstats:FindFirstChild("Time")
        if time then
            time.Value = 99999
        end
    end
    wait(1)
end
end)
