local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Polar's Hub", "DarkTheme")

-- MAIN TAB
local Main = Window:NewTab("Main")

-- MAIN SCRIPTS -- SECTION
local mainScripts = Tab:NewSection("Main Scripts")

-- MAIN SCRIPTS
MainSection:NewButton("WalkSpeed & JumpPower Modifier", "Opens A GUI, Which Allows You to Customly Modify Your WalkSpeed & JumpPower In-Game", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Godfly123456/Polar-Hub/main/Modifiers/WalkSpeed%20%26%20WalkPower%20GUI/GUI%20Loader", true))()
end)


-- MOBILE SCRIPTS -- SECTION
local mobileScripts = Tab:NewSection("Mobile Scripts")


-- MOBILE SCRIPTS
MainSection:NewButton("Keyboard Script", "Creats A Digital Keyboard On The User's Screen -- (Can Be Moved)", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
end)

-- MISCELLANEOUS -- SECTION
local miscScripts = Tab:NewSection("Miscellaneous")

-- MISC SCRIPTS
MainSection:NewButton("Time Modifier", "Sets The In-Game "Time" Leaderstat To 99,999 -- (May Reset After Leaderstat Is Updated)", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Godfly123456/Roblox-Add-Time-Script/main/conScript%20Loader", true))()
end)
